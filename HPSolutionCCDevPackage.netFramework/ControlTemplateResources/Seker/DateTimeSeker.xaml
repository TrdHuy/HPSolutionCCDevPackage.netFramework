                                               Foreground="#617187"
                                               Grid.Row="0"
                                               FontSize="15"
                                               Margin="14,0" />

                                    <Slider Grid.Column="2"
                                            Maximum="11"
                                            Grid.Row="1"
                                            Value="{Binding CurrentHour}"
                                            Style="{StaticResource SekerSlider}"
                                            Minimum="0"
                                            VerticalAlignment="Center" />

                                    <TextBlock Text="Minutes"
                                               Foreground="#617187"
                                               Grid.Row="2"
                                               FontSize="15"
                                               Margin="14,0" />

                                    <Slider Grid.Column="2"
                                            Maximum="59"
                                            Grid.Row="3"
                                            Value="{Binding CurrentMinute}"
                                            Style="{StaticResource SekerSlider}"
                                            Minimum="0"
                                            VerticalAlignment="Center" />

                                </Grid>

                                <!--AM & PM area-->
                                <StackPanel Grid.Row="2"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Center">
                                    <ToggleButton Style="{StaticResource AmPmTgBtStyle}"
                                                  IsChecked="{Binding CurrentTimeSession, Converter={StaticResource SekerTabIsCheckedConverter}, ConverterParameter=AM}"
                                                  Cursor="Hand"
                                                  FontWeight="Bold">
                                        <ToggleButton.Template>
                                            <ControlTemplate TargetType="{x:Type ButtonBase}">
                                                <Border x:Name="border"
                                                        BorderThickness="0"
                                                        CornerRadius="3,0,0,3"
                                                        Background="#D9DDE6"
                                                        SnapsToDevicePixels="True">
                                                    <ContentPresenter x:Name="contentPresenter"
                                                                      ContentTemplate="{TemplateBinding ContentTemplate}"
                                                                      Content="{TemplateBinding Content}"
                                                                      ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                                      Focusable="False"
                                                                      HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                                      Margin="{TemplateBinding Padding}"
                                                                      RecognizesAccessKey="True"
                                                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                                                </Border>

                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="Button.IsDefaulted"
                                                             Value="True">
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" />
                                                    </Trigger>
                                                    <Trigger Property="IsMouseOver"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#FFBEE6FD" />
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="#FF3C7FB1" />
                                                    </Trigger>
                                                    <Trigger Property="IsPressed"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#72C8FA" />
                                                    </Trigger>
                                                    <Trigger Property="ToggleButton.IsChecked"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#72C8FA" />
                                                        <Setter Property="Foreground"
                                                                Value="White" />
                                                    </Trigger>
                                                    <Trigger Property="IsEnabled"
                                                             Value="False">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#FFF4F4F4" />
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="#FFADB2B5" />
                                                        <Setter Property="Foreground"
                                                                Value="#FF838383" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </ToggleButton.Template>
                                        AM
                                    </ToggleButton>

                                    <ToggleButton Style="{StaticResource AmPmTgBtStyle}"
                                                  IsChecked="{Binding CurrentTimeSession, Converter={StaticResource SekerTabIsCheckedConverter}, ConverterParameter=PM}"
                                                  Cursor="Hand"
                                                  FontWeight="Bold">
                                        <ToggleButton.Template>
                                            <ControlTemplate TargetType="{x:Type ButtonBase}">
                                                <Border x:Name="border"
                                                        BorderThickness="0"
                                                        CornerRadius="0,3,3,0"
                                                        Background="#D9DDE6"
                                                        SnapsToDevicePixels="True">
                                                    <ContentPresenter x:Name="contentPresenter"
                                                                      ContentTemplate="{TemplateBinding ContentTemplate}"
                                                                      Content="{TemplateBinding Content}"
                                                                      ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                                      Focusable="False"
                                                                      HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                                      Margin="{TemplateBinding Padding}"
                                                                      RecognizesAccessKey="True"
                                                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="Button.IsDefaulted"
                                                             Value="True">
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" />
                                                    </Trigger>
                                                    <Trigger Property="IsMouseOver"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#FFBEE6FD" />
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="#FF3C7FB1" />
                                                    </Trigger>
                                                    <Trigger Property="IsPressed"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#72C8FA" />
                                                    </Trigger>
                                                    <Trigger Property="ToggleButton.IsChecked"
                                                             Value="True">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#72C8FA" />
                                                        <Setter Property="Foreground"
                                                                Value="White" />
                                                    </Trigger>
                                                    <Trigger Property="IsEnabled"
                                                             Value="False">
                                                        <Setter Property="Background"
                                                                TargetName="border"
                                                                Value="#FFF4F4F4" />
                                                        <Setter Property="BorderBrush"
                                                                TargetName="border"
                                                                Value="#FFADB2B5" />
                                                        <Setter Property="Foreground"
                                                                Value="#FF838383" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </ToggleButton.Template>
                                        PM
                                    </ToggleButton>
                                </StackPanel>

                            </Grid>
                            <Grid x:Name="PART_TimeSekerButton"
                                  Height="40"
                                  Background="{TemplateBinding Background}"
                                  Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1.8*" />
                                    <ColumnDefinition Width="1.1*" />
                                </Grid.ColumnDefinitions>


                                <Button x:Name="SEKER_NwBt"
                                        BorderThickness="0"
                                        Cursor="Hand"
                                        Background="Transparent">
                                    <Button.Template>

                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <Border Background="{TemplateBinding Background}"
                                                    BorderThickness="{TemplateBinding BorderThickness}">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="7*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Grid.Column="1"
                                                               Foreground="White"
                                                               FontWeight="Bold"
                                                               HorizontalAlignment="Center"
                                                               Text="Now"
                                                               VerticalAlignment="Center" />

                                                    <Rectangle Stroke="Transparent"
                                                               x:Name="HighlightRectangle"
                                                               Fill="Transparent"
                                                               Grid.ColumnSpan="3"
                                                               StrokeThickness="2">

                                                    </Rectangle>

                                                </Grid>

                                            </Border>

                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver"
                                                         Value="True">
                                                    <Setter Property="Stroke"
                                                            Value="#19000000"
                                                            TargetName="HighlightRectangle" />
                                                </Trigger>
                                                <Trigger Property="IsPressed"
                                                         Value="True">
                                                    <Setter Property="Fill"
                                                            Value="#19000000"
                                                            TargetName="HighlightRectangle" />
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Button.Template>
                                </Button>

                                <Button x:Name="SEKER_DnBt"
                                        BorderThickness="0"
                                        Command="{Binding DoneButtonCommand}"
                                        Grid.Column="1"
                                        Cursor="Hand"
                                        Background="#08D1C4">
                                    <Button.Template>

                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <Border Background="{TemplateBinding Background}"
                                                    BorderThickness="{TemplateBinding BorderThickness}">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="0.5*" />
                                                        <ColumnDefinition Width="1.6*" />
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="2*" />
                                                    </Grid.ColumnDefinitions>
                                                    <local:SquareGrid Grid.Column="1"
                                                                      IsTabStop="True"
                                                                      Margin="3">
                                                        <Viewbox Stretch="Uniform">
                                                            <Path Data="M 6.645784,17.868164 C 4.873034,14.880577 2.672909,12.110333 0.775185,10.476312 0.151206,9.9390389 0,9.6891129 0,9.1950189 c 0,-0.474415 0.26437,-0.999279 0.739038,-1.467243 0.701314,-0.691407 1.506963,-0.96381 2.038,-0.689078 0.284883,0.147383 1.684108,1.586857 2.312857,2.379386 0.429037,0.540794 1.219204,1.6860131 1.65826,2.4033781 l 0.175479,0.286712 0.122671,-0.202045 c 0.06747,-0.111125 0.335018,-0.564835 0.594556,-1.008246 2.2477,-3.8401181 5.764685,-7.2504181 9.758139,-9.4621301 0.919558,-0.50928201 3.081693,-1.49486701 3.14359,-1.43296901 0.01755,0.01755 -0.30158,0.293411 -0.709175,0.613029 C 18.731933,1.4795469 15.850975,4.4096269 14.645812,5.8918649 11.636589,9.5929339 9.048776,13.884519 7.551774,17.656497 l -0.413443,1.041748 z"
                                                                  Fill="White"
                                                                  Stroke="White" />
                                                        </Viewbox>
                                                    </local:SquareGrid>
                                                    <TextBlock Grid.Column="2"
                                                               Foreground="White"
                                                               FontWeight="Bold"
                                                               HorizontalAlignment="Center"
                                                               Text="Done     "
                                                               VerticalAlignment="Center" />
                                                    <Rectangle Stroke="Transparent"
                                                               x:Name="HighlightRectangle"
                                                               Fill="Transparent"
                                                               Grid.ColumnSpan="5"
                                                               StrokeThickness="2">

                                                    </Rectangle>
                                                </Grid>
                                            </Border>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver"
                                                         Value="True">
                                                    <Setter Property="Stroke"
                                                            Value="#19000000"
                                                            TargetName="HighlightRectangle" />
                                                </Trigger>
                                                <Trigger Property="IsPressed"
                                                         Value="True">
                                                    <Setter Property="Fill"
                                                            Value="#19000000"
                                                            TargetName="HighlightRectangle" />
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Button.Template>
                                </Button>
                            </Grid>
                        </Grid>
                    </local:ClippingBorder>

                    <ControlTemplate.Triggers>
                        <Trigger SourceName="SELECTOR_DteTgBt"
                                 Property="IsChecked"
                                 Value="True">
                            <Setter TargetName="PART_Calendar"
                                    Property="Visibility"
                                    Value="Visible" />
                            <Setter TargetName="PART_CalendarButton"
                                    Property="Visibility"
                                    Value="Visible" />
                            <Setter TargetName="PART_TimeSeker"
                                    Property="Visibility"
                                    Value="Hidden" />
                            <Setter TargetName="PART_TimeSekerButton"
                                    Property="Visibility"
                                    Value="Hidden" />
                        </Trigger>

                        <Trigger SourceName="SELECTOR_TmeTgBt"
                                 Property="IsChecked"
                                 Value="True">
                            <Setter TargetName="PART_Calendar"
                                    Property="Visibility"
                                    Value="Hidden" />
                            <Setter TargetName="PART_CalendarButton"
                                    Property="Visibility"
                                    Value="Hidden" />
                            <Setter TargetName="PART_TimeSeker"
                                    Property="Visibility"
                                    Value="Visible" />
                            <Setter TargetName="PART_TimeSekerButton"
                                    Property="Visibility"
                                    Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>